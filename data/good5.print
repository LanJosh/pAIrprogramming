program
  int n;
  int a,b;
  int isPrime;
begin
  input n;
  while !(n=0) begin
    if 1<n then
      a:=2;
      b:=n;
      isPrime:=1;
      while a<=b-1 begin
        while n<a*b begin
          b:=b-1;
        endwhile;
        if a*b=n then
          isPrime:=0;
        endif;
        a:=a+1;
      endwhile;
      if isPrime=1 then
        output n;
      else 
        output 0-n;
      endif;
    endif;
    input n;
  endwhile;
end
